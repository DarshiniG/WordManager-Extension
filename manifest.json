{

"name": "Word Manager",
"description": "To highlight and store important points and to find synonyms of word",
"version":"1.0",

"manifest_version":2,

"browser_action":{
  "default_icon":"icon.png",
  "default_popup":"popup.html"
},

"permissions":[
  "storage",
  "contextMenus",
  "clipboardRead",
	"clipboardWrite",
	"http://thesaurus.altervista.org/thesaurus/*"
],

"background": {
    "scripts": [
    "popup.js",
	
]
},

"icons": {
     "128": "icon128.png"
    , "16": "icon.png"
    , "32": "icon19.png"
    , "48": "icon48.png"
}










}